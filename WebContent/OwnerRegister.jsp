             <form action="OwnerRegisterServlet" method="post" >
           
          <input type="text" name="ownername" class="form-control" id="ownername"   placeholder="Enter Your Name" required>
          <br>
           <input type="text" name="address" class="form-control" id="address" placeholder="Your address" required>
           <br>
              <input type="date" name="dob" class="form-control" id="dob" placeholder="Your Date of Birth" required>
           <br>
              <input type="text" name="mobileno" class="form-control" id="mobileno" required pattern="[7-9]{1}[0-9]{9}" placeholder="Your Mobile Number" required>
          <br>
              <input type="email" class="form-control" name="email" id="email" placeholder="Your Email" required>
            <br>
              <input type="password" name="password" class="form-control" id="password" required pattern="(?=.*\d)(?=.*[a-z])(?=.*[A-Z]).{6,}" placeholder="Your password" required>
          <br>
          <label>Add bank Details</label>
          <input type="text" name="bankname" class="form-control" id="bankname" placeholder="Bank Name" required>
           <br>
           <input type="text" name="accountno" class="form-control" id="accountno" placeholder="Account No" required>
           <br>
              <input type="text" name="ifsc" class="form-control" id="ifsc" placeholder="IFSC Code" required>
           <br>
              <input type="text" name="bankaddr" class="form-control" id="bankaddr" required  placeholder="Bank Address" required>
